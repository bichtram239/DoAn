    Layout = "~/Views/Shared/_LayoutTaikhoan.cshtml";
}
@using (Html.BeginForm())
{
    <span class="login100-form-title p-b-33">
        ĐĂNG NHẬP
    </span>
    @Html.ValidationSummary(true)
    <div class="wrap-input100 validate-input" style="">

        <input class="input100" type="text" name="tendn" placeholder="Tài khoản">
        <font color="red"> @ViewData["Loi1"]</font>
        
    </div>

    <div class="wrap-input100 rs1 validate-input">
        <input class="input100" type="password" name="matkhau" placeholder="Mật khẩu">
